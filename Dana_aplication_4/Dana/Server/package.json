{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:sonar": "jest --coverage --testResultsProcessor=jest-sonar-reporter",
    "test-coverage": "react-scripts test --coverageDirectory test-coverage --coverage --watchAll=false"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mongodb": "^6.12.0",
    "mongodb-memory-server": "^8.0.0",
    "mongoose": "^8.8.4",
    "mysql": "^2.18.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/test/**",
      "!**/coverage/**",
      "!**/dist/**"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "coverageReporters": [
      "lcov",
      "text",
      "cobertura"
    ]
  },
  "devDependencies": {
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "supertest": "^7.0.0"
  },
  "jestSonar": {
    "reportPath": "./coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  }
}

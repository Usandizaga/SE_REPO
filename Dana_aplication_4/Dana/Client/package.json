{
  "name": "dana",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:sonar": "jest --coverage --testResultsProcessor=jest-sonar-reporter",
    "test-coverage": "react-scripts test --coverageDirectory test-coverage --coverage --watchAll=false"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "util": "^0.12.5",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "mongodb-memory-server": "^10.1.3",
    "mongoose": "^8.8.4",
    "supertest": "^7.0.0",
    "vite": "^6.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/test/**",
      "!**/coverage/**",
      "!**/dist/**"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "coverageReporters": [
      "lcov",
      "text",
      "cobertura"
    ],
    "setupFiles": [
      "<rootDir>/setupTest.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "jestSonar": {
    "reportPath": "./coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  }
}
